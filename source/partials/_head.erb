<html>
	<head>
		<title><%= data.site.sitename %><%= ' - ' + current_page.data.title unless current_page.data.title.nil? %></title>
		<meta charset="utf-8" />
		<meta name="viewport" content="width=device-width, initial-scale=1" />
		<!--[if lte IE 8]><script src="/assets/js/ie/html5shiv.js"></script><![endif]-->
		<link rel="stylesheet" href="/assets/css/main.css" />
		<!--[if lte IE 8]><link rel="stylesheet" href="/assets/css/ie8.css" /><![endif]-->
      <meta property="og:type" content="article">
	   <meta property="og:title" content="<%= data.site.sitename %> - <%= current_page.data.title || data.site.subtitle %>">
	   <% if current_page.data.summary %>
	   <meta property="og:description" content="<%= current_page.data.summary %>">
	   <meta property="og:summary" content="<%= current_page.data.summary %>">
	   <% end %>
	   <% if current_page.data.feature_image %>
   	<meta property="og:image" content="<%= data.site.siteurl %>/images/<%= current_page.data.feature_image %>">
   	<% end %>
   	<% unless current_page.data.date.nil? %>
      <meta property="article:published_time" content="<%= current_page.data.date %>">
      <% end %>
	</head>